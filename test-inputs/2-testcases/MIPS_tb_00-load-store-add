P_0x55682624fc20 .param/str "RAM_INIT_FILE" 0 4 9, "test-inputs/1-binary/00-load-store-add.hex.txt";
v0x55682624a7c0_0 .net "address", 11 0, v0x5568262a51b0_0;  1 drivers
