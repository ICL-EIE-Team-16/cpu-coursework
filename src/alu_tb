    %load/vec4 v0x7fffc8f36eb0_0;
    %load/vec4 v0x7fffc8f36c60_0;
    %load/vec4 v0x7fffc8f36d40_0;
    %add;
    %cmp/e;
    %jmp/0xz  T_1.0, 4;
    %jmp T_1.1;
T_1.0 ;
