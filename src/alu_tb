    .thread T_0, $push;
    .scope S_0x7fffc8f12f20;
T_1 ;
    %pushi/vec4 8, 0, 32;
    %cassign/vec4 v0x7fffc8f36c60_0;
    %pushi/vec4 15, 0, 32;
