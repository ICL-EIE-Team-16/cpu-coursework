v0x5568262a4f50_0 .net "write", 0 0, v0x5568262a54a0_0;  1 drivers
v0x5568262a5010_0 .net "writedata", 31 0, v0x5568262a5590_0;  1 drivers
E_0x556826219530 .event posedge, v0x556826267080_0;
S_0x5568262999e0 .scope begin, "$unm_blk_50" "$unm_blk_50" 4 13, 4 13 0, S_0x556826209f50;
 .timescale 0 0;
v0x556826299f90_0 .var/i "i", 31 0;
    .scope S_0x556826209f50;
T_0 ;
    %fork t_1, S_0x5568262999e0;
    %jmp t_0;
    .scope S_0x5568262999e0;
t_1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x556826299f90_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x556826299f90_0;
    %cmpi/s 4096, 0, 32;
    %jmp/0xz T_0.1, 5;
    %pushi/vec4 0, 0, 32;
    %ix/getv/s 4, v0x556826299f90_0;
    %store/vec4a v0x5568262a4d10, 4, 0;
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x556826299f90_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0x556826299f90_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %vpi_call/w 4 21 "$display", "RAM : INIT : Loading RAM contents from %s", P_0x55682624fc20 {0 0 0};
    %vpi_call/w 4 22 "$readmemh", P_0x55682624fc20, v0x5568262a4d10 {0 0 0};
    %end;
    .scope S_0x556826209f50;
t_0 %join;
    %end;
    .thread T_0;
    .scope S_0x556826209f50;
T_1 ;
    %wait E_0x556826219530;
    %load/vec4 v0x55682624a7c0_0;
    %pad/u 14;
    %ix/vec4 4;
    %load/vec4a v0x5568262a4d10, 4;
    %vpi_call/w 4 28 "$display", "RAM : INFO : read=%h, addr = %h, mem=%h", v0x5568262a4db0_0, v0x55682624a7c0_0, S<0,vec4,u32> {1 0 0};
    %load/vec4 v0x5568262a4f50_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x5568262a5010_0;
    %load/vec4 v0x55682624a7c0_0;
    %pad/u 14;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x5568262a4d10, 0, 4;
T_1.0 ;
    %load/vec4 v0x55682624a7c0_0;
    %pad/u 14;
    %ix/vec4 4;
    %load/vec4a v0x5568262a4d10, 4;
    %assign/vec4 v0x5568262a4e70_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x556826209d20;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5568262a5290_0, 0, 1;
    %pushi/vec4 10000, 0, 32;
T_2.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.1, 5;
    %jmp/1 T_2.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 1000, 0;
    %load/vec4 v0x5568262a5290_0;
    %nor/r;
    %store/vec4 v0x5568262a5290_0, 0, 1;
    %delay 1000, 0;
    %load/vec4 v0x5568262a5290_0;
    %nor/r;
    %store/vec4 v0x5568262a5290_0, 0, 1;
    %jmp T_2.0;
T_2.1 ;
    %pop/vec4 1;
    %vpi_call/w 3 31 "$fatal", 32'sb00000000000000000000000000000010, "Simulation did not finish within %d cycles.", P_0x5568261cd590 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x556826209d20;
T_3 ;
    %delay 500, 0;
    %pushi/vec4 0, 0, 12;
    %store/vec4 v0x5568262a51b0_0, 0, 12;
    %delay 2000, 0;
    %vpi_call/w 3 39 "$display", "Memory OUT: %h", v0x5568262a53d0_0 {0 0 0};
    %pushi/vec4 1, 0, 12;
    %store/vec4 v0x5568262a51b0_0, 0, 12;
    %delay 2000, 0;
    %vpi_call/w 3 44 "$display", "Memory OUT: %h", v0x5568262a53d0_0 {0 0 0};
    %pushi/vec4 2, 0, 12;
    %store/vec4 v0x5568262a51b0_0, 0, 12;
    %delay 2000, 0;
